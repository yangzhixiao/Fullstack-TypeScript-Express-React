  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "antd": "^3.11.6",
    "express": "^4.17.1",
    "mobx": "^5.10.0",
    "mobx-react": "^6.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "scripts": {
    "clear": "rm -rf ./public/index.html && rm -rf ./public/bundle.*.js",
    "dev": "rm -rf ./build/* && cross-env NODE_ENV=development ./node_modules/.bin/ts-node index.ts",
    "copy": "mkdir -p ./public && cp ./node_modules/antd/dist/antd.min.css ./public && cp ./build/* ./public",
    "deploy": "yarn clear && cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js --progress && yarn copy && rm -rf ./build/*",
    "prod": "cross-env NODE_ENV=production ./node_modules/.bin/ts-node index.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "typescript": "3.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
